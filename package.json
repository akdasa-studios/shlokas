{
  "name": "@akdasa-studios/shlokas",
  "version": "1.2.3",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "appflow:build": "if [ \"$CI_PLATFORM\" != \"web\" ]; then npx trapeze run appflow.yml -y --$CI_PLATFORM; fi && npm run build",
    "build": "vue-tsc && vite build --mode prod",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:prod": "vue-tsc && vite build --mode prod",
    "build:testing:dev": "vue-tsc && vite build --mode testing:dev",
    "build:testing:prod": "vue-tsc && vite build --mode testing:prod",
    "ionic:serve": "vite dev --host",
    "ionic:build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "run:device": "npm run build:dev && npx cap run ios",
    "run:device:hot": "npx ionic capacitor run ios --livereload --external",
    "gen:resources": "npx capacitor-assets generate --assetPath 'resources' --ios --android",
    "lint": "eslint"
  },
  "dependencies": {
    "@akdasa-studios/shlokas-core": "1.1.18",
    "@akdasa-studios/shlokas-protocol": "^0.0.6",
    "@akdasa-studios/shlokas-uikit": "0.0.19",
    "@awesome-cordova-plugins/sign-in-with-apple": "^6.3.0",
    "@capacitor-community/apple-sign-in": "^4.0.0",
    "@capacitor/android": "4.7.3",
    "@capacitor/app": "4.1.1",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/core": "4.7.3",
    "@capacitor/device": "^4.1.0",
    "@capacitor/filesystem": "^4.1.4",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.7.3",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/splash-screen": "^4.1.4",
    "@capacitor/status-bar": "4.1.1",
    "@capawesome/capacitor-background-task": "^2.0.0",
    "@codetrix-studio/capacitor-google-auth": "^3.2.2",
    "@ionic/core": "^7.0.6",
    "@ionic/storage": "^3.0.6",
    "@ionic/vue": "^7.0.2",
    "@ionic/vue-router": "^7.0.2",
    "@sentry/capacitor": "^0.11.2",
    "@sentry/vue": "7.37.1",
    "@trapezedev/configure": "^7.0.10",
    "@vueuse/core": "^9.13.0",
    "capacitor-blob-writer": "^1.1.9",
    "cordova-plugin-ionic": "5.5.3",
    "cordova-plugin-sign-in-with-apple": "^0.1.2",
    "cordova-sqlite-storage": "^6.1.0",
    "eventemitter2": "^6.4.9",
    "ionicons": "^7.1.0",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.33",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-cordova-sqlite": "^2.0.8",
    "pouchdb-find": "^8.0.1",
    "pouchdb-upsert": "^2.2.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/cli": "4.7.3",
    "@ionic/cli": "7.1.1",
    "@types/node": "^18.11.1",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-upsert": "^2.2.6",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "eslint": "^8.35.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "description": "Shlokas"
}
