{
  "name": "@akdasa-studios/shlokas",
  "version": "1.2.3",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "appflow:build": "if [ \"$CI_PLATFORM\" != \"web\" ]; then npx trapeze run appflow.yml -y --$CI_PLATFORM; fi && npm run build",
    "build": "vue-tsc && vite build --mode prod",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:prod": "vue-tsc && vite build --mode prod",
    "build:testing:dev": "vue-tsc && vite build --mode testing:dev",
    "build:testing:prod": "vue-tsc && vite build --mode testing:prod",
    "ionic:serve": "vite dev --host",
    "ionic:build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "run:device": "npm run build:prod && npx cap run ios",
    "run:device:hot": "npx ionic capacitor run ios --livereload --external",
    "gen:resources": "npx capacitor-assets generate --assetPath 'resources' --ios --android",
    "lint": "eslint"
  },
  "dependencies": {
    "@akdasa-studios/shlokas-core": "1.1.19",
    "@akdasa-studios/shlokas-protocol": "^0.0.6",
    "@akdasa-studios/shlokas-uikit": "0.0.19",
    "@awesome-cordova-plugins/core": "6.3.0",
    "@awesome-cordova-plugins/email-composer": "^6.3.0",
    "@awesome-cordova-plugins/sign-in-with-apple": "^6.3.0",
    "@capacitor-community/apple-sign-in": "5.0.0",
    "@capacitor-community/sqlite": "5.0.0",
    "@capacitor/android": "^5.0.4",
    "@capacitor/app": "^5.0.2",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/core": "^5.0.4",
    "@capacitor/device": "^5.0.2",
    "@capacitor/filesystem": "^5.0.2",
    "@capacitor/haptics": "^5.0.2",
    "@capacitor/ios": "^5.0.4",
    "@capacitor/keyboard": "^5.0.2",
    "@capacitor/splash-screen": "^5.0.2",
    "@codetrix-studio/capacitor-google-auth": "^3.3.0",
    "@ionic/core": "^7.0.9",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^7.0.9",
    "@ionic/vue-router": "^7.0.9",
    "@sentry/capacitor": "^0.11.4",
    "@sentry/replay": "7.50.0",
    "@sentry/vue": "7.50.0",
    "@trapezedev/configure": "^7.0.10",
    "@vueuse/core": "^10.1.2",
    "capacitor-blob-writer": "^1.1.10",
    "cordova-plugin-email-composer": "^0.10.1",
    "cordova-plugin-ionic": "5.5.3",
    "cordova-plugin-sign-in-with-apple": "^0.1.2",
    "eventemitter2": "^6.4.9",
    "ionicons": "^7.1.1",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.1.3",
    "pouchdb": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "pouchdb-upsert": "^2.2.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.4",
    "@ionic/cli": "7.1.1",
    "@types/node": "^18.11.1",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-upsert": "^2.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.41.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.14.1",
    "jsdom": "^22.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  },
  "description": "Shlokas"
}
